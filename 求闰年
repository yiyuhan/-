#include<stdio.h>

int main()
{

    int year=0;

    while(year<=1500)
    {
        if(year%4==0)
        {
            if(year%100==0)
            {
                if(year%400==0)
                    printf("是闰年%d\n",year);
                else
                    printf("不是闰年%d\n",year);
            }
            else
                printf("是闰年%d\n",year);
        }
        else
            printf("不是闰年%d\n",year);

        year++;
    }

    return 0;
}
